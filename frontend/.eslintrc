  "parserOptions": {
    "requireConfigFile" : false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "extends": ["plugin:prettier/recommended"]
}